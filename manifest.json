{
    "name": "Quizlet Margin",
    "description": "Margin vertical of Quizlet since it's have a space blocked by Adblock",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "js": ["margin.js"],
            "css": ["effect.css"],
            "matches": ["https://quizlet.com/*/flashcards"]
        }
    ],
    "action": {
        "default_icon": {
            "16": "/images/16.png",
            "32": "/images/32.png",
            "48": "/images/48.png",
            "128": "/images/128.png"
        }
    },
    "icons": {
            "16": "/images/16.png",
            "32": "/images/32.png",
            "48": "/images/48.png",
            "128": "/images/128.png"
    }
}